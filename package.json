{
  "name": "react-ui-components",
  "version": "1.0.0",
  "description": "react ui components",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack serve --config=webpack.config.ts --port=9001 --host=0.0.0.0 --compress --open --history-api-fallback",
    "test": "jest",
    "build": "webpack --config=webpack.config.ts",
    "watch": "tsc --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && pretty-quick --staged"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*/__tests__/.+\\.(generator|test|spec)\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reeli/react-ui-components.git"
  },
  "author": "reeli",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reeli/react-ui-components/issues"
  },
  "homepage": "https://github.com/reeli/react-ui-components#readme",
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "7.12.1",
    "@babel/plugin-syntax-typescript": "7.12.1",
    "@babel/plugin-transform-typescript": "7.12.1",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@emotion/babel-preset-css-prop": "11.0.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@types/d3-shape": "2.0.0",
    "@types/draft-js": "0.10.44",
    "@types/html-webpack-plugin": "^3.2.5",
    "@types/invariant": "2.2.34",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/moment": "2.13.0",
    "@types/node": "14.14.22",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-router": "5.1.11",
    "@types/react-router-dom": "5.1.7",
    "@types/webpack": "4.41.26",
    "@webpack-cli/serve": "^1.2.2",
    "babel-loader": "8.2.2",
    "babel-plugin-emotion": "11.0.0",
    "babel-plugin-typescript-iife-enum": "0.2.1",
    "d3-shape": "2.0.0",
    "draft-js": "0.11.7",
    "html-webpack-plugin": "4.5.1",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "raw-loader": "4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "webpack": "5.18.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "date-fns": "^2.23.0",
    "invariant": "^2.2.4",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "react-spring": "^9.1.0",
    "react-use-gesture": "^9.1.3"
  },
  "peerDependencies": {
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1"
  }
}
