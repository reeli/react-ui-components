{
  "name": "react-ui-components",
  "version": "1.0.0",
  "description": "react ui components",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --config=webpack.config.ts --port=9001 --compress --open --history-api-fallback",
    "test": "jest",
    "build": "webpack --config=webpack.config.ts",
    "watch": "tsc --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && pretty-quick --staged"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*/__tests__/.+\\.(generator|test|spec)\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reeli/react-ui-components.git"
  },
  "author": "reeli",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reeli/react-ui-components/issues"
  },
  "homepage": "https://github.com/reeli/react-ui-components#readme",
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-syntax-typescript": "7.8.3",
    "@babel/plugin-transform-typescript": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@emotion/babel-preset-css-prop": "10.0.27",
    "@types/d3-shape": "1.3.2",
    "@types/draft-js": "0.10.38",
    "@types/html-webpack-plugin": "3.2.2",
    "@types/invariant": "2.2.31",
    "@types/jest": "25.1.3",
    "@types/lodash": "4.14.149",
    "@types/moment": "2.13.0",
    "@types/node": "13.7.4",
    "@types/react": "16.9.22",
    "@types/react-dom": "16.9.5",
    "@types/react-router": "5.1.4",
    "@types/react-router-dom": "5.1.3",
    "@types/webpack": "4.41.6",
    "babel-loader": "8.0.6",
    "babel-plugin-emotion": "10.0.27",
    "babel-plugin-typescript-iife-enum": "0.2.1",
    "html-webpack-plugin": "4.0.0-alpha",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "raw-loader": "4.0.0",
    "ts-jest": "25.2.1",
    "ts-node": "8.6.2",
    "typescript": "3.8.2",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "dependencies": {
    "@emotion/core": "10.0.27",
    "@material-ui/core": "4.9.4",
    "@material-ui/icons": "4.9.1",
    "d3-shape": "1.3.7",
    "draft-js": "0.11.4",
    "draft-js-mention-plugin": "3.1.5",
    "draft-js-plugins-editor": "3.0.0",
    "invariant": "2.2.4",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2"
  }
}
