{
  "name": "react-ui-components",
  "version": "1.0.0",
  "description": "react ui components",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --config=webpack.config.ts --port=9001 --compress --open --history-api-fallback",
    "test": "jest",
    "pretty": "prettier --write '{src,app-style-guide,__{tests,mocks}__}/**/*{.ts,tsx}'",
    "build": "webpack --config=webpack.config.ts",
    "watch": "tsc --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*/__tests__/.+\\.(generator|test|spec)\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reeli/react-ui-components.git"
  },
  "author": "reeli",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reeli/react-ui-components/issues"
  },
  "homepage": "https://github.com/reeli/react-ui-components#readme",
  "devDependencies": {
    "@babel/core": "7.7.5",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "7.7.4",
    "@babel/plugin-transform-typescript": "7.7.4",
    "@babel/preset-env": "7.4.3",
    "@emotion/babel-preset-css-prop": "10.0.9",
    "@types/d3-shape": "1.2.7",
    "@types/draft-js": "0.10.34",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/invariant": "2.2.29",
    "@types/jest": "24.0.22",
    "@types/lodash": "4.14.119",
    "@types/moment": "2.13.0",
    "@types/node": "12.12.6",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "@types/react-router": "4.4.3",
    "@types/react-router-dom": "4.3.1",
    "@types/webpack": "4.4.22",
    "babel-loader": "8.0.6",
    "babel-plugin-typescript-iife-enum": "0.1.0",
    "html-webpack-plugin": "4.0.0-alpha",
    "husky": "1.3.1",
    "jest": "24.9.0",
    "prettier": "1.15.3",
    "pretty-quick": "1.8.0",
    "raw-loader": "1.0.0",
    "ts-jest": "24.1.0",
    "ts-node": "8.0.3",
    "typescript": "3.7.2",
    "webpack": "4.28.3",
    "webpack-cli": "3.2.0",
    "webpack-dev-server": "3.1.14"
  },
  "dependencies": {
    "@emotion/core": "10.0.10",
    "@material-ui/core": "4.7.2",
    "@material-ui/icons": "4.5.1",
    "d3-shape": "1.2.2",
    "draft-js": "0.10.5",
    "draft-js-mention-plugin": "3.1.3",
    "draft-js-plugins-editor": "2.1.1",
    "glamor": "2.20.40",
    "invariant": "2.2.4",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1"
  }
}
